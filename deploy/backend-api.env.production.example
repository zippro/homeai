# Runtime mode (must be production on live server)
APP_ENV=production
ALLOW_OPEN_ADMIN_MODE=false

# Core
DATABASE_URL=postgresql+psycopg://homeai_prod:CHANGE_ME@prod-db.example.com:5432/homeai_prod
ALLOWED_ORIGINS=https://app.homeai.example.com,https://admin.homeai.example.com

# fal provider
FAL_API_KEY=
FAL_QUEUE_BASE=https://queue.fal.run
FAL_TIMEOUT_SECONDS=45

# OpenAI provider
OPENAI_API_KEY=
OPENAI_API_BASE=https://api.openai.com/v1
OPENAI_STUB_IF_MISSING_KEY=false
OPENAI_TIMEOUT_SECONDS=60

# Object storage
STORAGE_BUCKET=homeai-prod-outputs
STORAGE_REGION=us-east-1
STORAGE_ENDPOINT_URL=
STORAGE_ACCESS_KEY_ID=
STORAGE_SECRET_ACCESS_KEY=
STORAGE_PUBLIC_BASE_URL=https://cdn.example.com/homeai

# Billing + webhooks
STOREKIT_WEBHOOK_SECRET=
GOOGLE_PLAY_WEBHOOK_SECRET=
WEB_BILLING_WEBHOOK_SECRET=
WEB_BILLING_CHECKOUT_BASE_URL=https://payments.example.com/checkout

# Admin auth
ADMIN_API_TOKEN=
ADMIN_USER_IDS=

# Experiment automation notifications
EXPERIMENT_AUTOMATION_NOTIFY_WEBHOOK_URL=
